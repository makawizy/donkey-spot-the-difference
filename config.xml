<?xml version="1.0" encoding="utf-8"?>
<widget id="com.donkeytech.spotthedifference" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="android-windowSoftInputMode" value="adjustResize" />
    <name>Spot The Difference</name>
    <description> A sample Apache Cordova application that responds to the deviceready event.
</description>
<icon src="icon.png" />
    <author email="dev@cordova.apache.org" href="http://cordova.io"> Apache Cordova Team
</author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="LowLatencyAudio">
        <param name="android-package" value="com.phonegap.PGLowLatencyAudio" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.plugin.AdMob" />
    </feature>
</widget>